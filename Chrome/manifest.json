{
    "name": "Bolha Eliminator",
    "description": "Eliminiranje oglasov, ki ponujajo odkup na strani bolha.com",
    "version": "1.1.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://www.bolha.com/*"],
            "js": ["/content-script.js"]
        }
    ],
    "icons": {
        "128": "/ikona.png"
      },
    "action": {
        "default_popup": "popup.html"
      },
      
    "permissions": [
        "storage"
      ]
  }